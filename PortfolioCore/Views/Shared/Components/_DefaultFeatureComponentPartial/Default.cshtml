@model List<Feature>

<section id="hero" class="hero section light-background">

	<img src="/MyResume-1.0.0/assets/img/viewresim.jpg" alt="">

	<div class="container" data-aos="zoom-out">
		<div class="row justify-content-center">
			<div class="col-lg-9">
				<h2>Münire Gamze Zeren</h2>
				@foreach (var item in Model)
				{
					<p>Çalışma Alanlarım: <span class="typed" data-typed-items="@item.Title1, @item.Title2, @item.Title3"></span><span class="typed-cursor typed-cursor--blink" aria-hidden="true"></span></p>
				}
				@await Component.InvokeAsync("_DefaultSocialMediaComponentPartial")

			</div>
		</div>
	</div>

</section>
<!-- /Hero Section -->